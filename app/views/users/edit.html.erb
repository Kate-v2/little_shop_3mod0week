<h1>Please enter your new info here</h1>
<%= form_for @user do |f| %>
  <p><%= f.label :name %>
  <%= f.text_field :name %></p>
  <p><%= f.label :address %>
  <%= f.text_field :address %></p>
  <p><%= f.label :city %>
  <%= f.text_field :city %></p>
  <p><%= f.label :state %>
  <%= f.text_field :state %></p>
  <p><%= f.label :zip %>
  <%= f.number_field :zip %></p>
  <p><%= f.label :email %>
  <%= f.text_field :email %></p>
  <p><%= f.submit %></p>
<% end %>
